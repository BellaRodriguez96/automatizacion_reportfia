            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import java.net.URLDecoder
import javax.net.ssl.HttpsURLConnection
import java.security.cert.X509Certificate

// =========================
// XSRF COOKIE
// =========================
String xsrf = vars.get(&quot;XSRF_COOKIE&quot;)
if (!xsrf) {
    vars.put(&quot;XSRF_COOKIE_FORMAT&quot;, &quot;NO_VALIDO&quot;)
} else {
    vars.put(&quot;XSRF_COOKIE_FORMAT&quot;, URLDecoder.decode(xsrf, &quot;UTF-8&quot;))
}

// =========================
// CSRF TOKEN
// =========================
String csrf = vars.get(&quot;CSRF_TOKEN&quot;)
if (!csrf) {
    vars.put(&quot;CSRF_TOKEN_FORMAT&quot;, &quot;NO_VALIDO&quot;)
} else {
    vars.put(&quot;CSRF_TOKEN_FORMAT&quot;, URLDecoder.decode(csrf, &quot;UTF-8&quot;))
}

// =========================
// CERTIFICADO HTTPS
// =========================
def conn = prev.getConnection()

if (conn instanceof HttpsURLConnection) {
    try {
        def certs = conn.getServerCertificates()

        if (certs &amp;&amp; certs[0] instanceof X509Certificate) {
            X509Certificate cert = certs[0]

            vars.put(&quot;cert_subject&quot;, cert.getSubjectDN().toString())
            vars.put(&quot;cert_issuer&quot;, cert.getIssuerDN().toString())
            vars.put(&quot;cert_start&quot;, cert.getNotBefore().toString())
            vars.put(&quot;cert_end&quot;, cert.getNotAfter().toString())

            boolean valido = true
            try { cert.checkValidity() }
            catch (Exception e) { valido = false }

            vars.put(&quot;cert_valid&quot;, valido.toString())
        }
    } catch (Exception e) {
        vars.put(&quot;cert_valid&quot;, &quot;NO_VALIDO&quot;)
    }
} else {
    vars.put(&quot;cert_valid&quot;, &quot;NO_VALIDO&quot;)
}
</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
          </JSR223PreProcessor>
          <hashTree/>
          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="JSR223 PostProcessor">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import org.apache.jmeter.protocol.http.sampler.HTTPHC4Impl
import javax.net.ssl.HttpsURLConnection

def sampler = ctx.getCurrentSampler()
log.info(cert_valid)
// Verifica que el sampler fue ejecutado por HttpClient4
if (!(sampler instanceof HTTPHC4Impl)) {
    log.warn(&quot;Sampler NO usa HttpClient4 → no se puede leer certificado SSL.&quot;)
    vars.put(&quot;cert_valid&quot;, &quot;NO_VALIDO&quot;)
    return
}

def conn = sampler.getConnection()

if (!conn || !(conn instanceof HttpsURLConnection)) {
    log.warn(&quot;No se obtuvo conexión HTTPS válida.&quot;)
    vars.put(&quot;cert_valid&quot;, &quot;NO_VALIDO&quot;)
    return
}

def certs = conn.getServerCertificates()

if (!certs || certs.size() == 0) {
    vars.put(&quot;cert_valid&quot;, &quot;NO_VALIDO&quot;)
    log.warn(&quot;El servidor no envió certificados.&quot;)
    return
}

def cert = certs[0]

// Guardar información
vars.put(&quot;cert_subject&quot;, cert.getSubjectDN().toString())
vars.put(&quot;cert_issuer&quot;, cert.getIssuerDN().toString())
vars.put(&quot;cert_start&quot;, cert.getNotBefore().toString())
vars.put(&quot;cert_end&quot;, cert.getNotAfter().toString())

try {
    cert.checkValidity()
    vars.put(&quot;cert_valid&quot;, &quot;VALIDO&quot;)
} catch (Exception e) {
    vars.put(&quot;cert_valid&quot;, &quot;NO_VALIDO&quot;)
}

log.info(&quot;Certificado extraído correctamente.&quot;)
</stringProp>
