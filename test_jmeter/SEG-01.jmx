          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">def sampler = ctx.getCurrentSampler()
def cookieManager = sampler ? sampler.getCookieManager() : null

if (cookieManager) {

    def cookies = cookieManager.cookies

    for (int i = cookies.size() - 1; i &gt;= 0; i--) {
        def c = cookies.get(i)
        if (c.name.contains(&quot;remember_web&quot;)) {
            cookies.remove(i)
            log.info(&quot;Cookie remember_web eliminada (índice ${i}).&quot;)
        }
    }

} else {
    log.warn(&quot;No se encontró CookieManager en el sampler actual.&quot;)
}
</stringProp>
