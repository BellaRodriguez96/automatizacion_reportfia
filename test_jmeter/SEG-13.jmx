          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import org.apache.jmeter.protocol.http.sampler.HTTPSamplerBase

def sampler = ctx.getCurrentSampler()

// Si el sampler NO es HTTP, salir sin hacer nada
if (!(sampler instanceof HTTPSamplerBase)) {
    log.info(&quot;JSR223 PreProcessor: Sampler no HTTP, se omite limpieza de cookies.&quot;)
    return
}

def cookieManager = sampler.getCookieManager()

if (cookieManager == null) {
    log.warn(&quot;JSR223 PreProcessor: No existe CookieManager.&quot;)
    return
}

def cookies = cookieManager.cookies

for (int i = cookies.size() - 1; i &gt;= 0; i--) {
    def c = cookies.get(i)
    if (c.name.contains(&quot;remember_web&quot;)) {
        cookies.remove(i)
        log.info(&quot;Cookie remember_web eliminada.&quot;)
    }
}
</stringProp>
