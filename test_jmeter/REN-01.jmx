          <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="JSR223 PreProcessor" enabled="true">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import java.net.URLDecoder;

// ========================
// Manejo de XSRF_COOKIE
// ========================
String encodedXsrf = vars.get(&quot;XSRF_COOKIE&quot;);

if (encodedXsrf == null || encodedXsrf.isEmpty()) {
    log.warn(&quot;XSRF_COOKIE is null or empty!&quot;);
    vars.put(&quot;XSRF_COOKIE_FORMAT&quot;, &quot;&quot;); 
} else {
    String decodedXsrf = URLDecoder.decode(encodedXsrf, &quot;UTF-8&quot;);
    vars.put(&quot;XSRF_COOKIE_FORMAT&quot;, decodedXsrf);
}

// ========================
// Manejo de CSRF_TOKEN
// ========================
String encodedCsrf = vars.get(&quot;CSRF_TOKEN&quot;);

if (encodedCsrf == null || encodedCsrf.isEmpty()) {
    log.warn(&quot;CSRF_TOKEN is null or empty!&quot;);
    vars.put(&quot;CSRF_TOKEN_FORMAT&quot;, &quot;&quot;); 
} else {
    String decodedCsrf = URLDecoder.decode(encodedCsrf, &quot;UTF-8&quot;);
    vars.put(&quot;CSRF_TOKEN_FORMAT&quot;, decodedCsrf);
}</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
